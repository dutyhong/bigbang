count ( case when use_time_3 and init_type_1 and months_6 then start_time else null end ) as c_use_time_3_init_type_1_months_6_start_time,
count ( case when use_time_4 then start_time else null end ) as c_use_time_4_start_time,
count (distinct  case when use_time_4 and init_type_1 and months_6 then other_cell_phone else null end ) as cd_use_time_4_init_type_1_months_6_other_cell_phone,
count (distinct  case when use_time_8 and init_type_2 and months_2 then other_cell_phone else null end ) as cd_use_time_8_init_type_2_months_2_other_cell_phone,
count (distinct  case when use_time_8 then other_cell_phone else null end ) as cd_use_time_8_other_cell_phone,
count (distinct  case when use_time_1 and months_1 then other_cell_phone else null end ) as cd_use_time_1_months_1_other_cell_phone,
count (distinct  case when use_time_6 and months_5 then other_cell_phone else null end ) as cd_use_time_6_months_5_other_cell_phone,
count ( case when use_time_11 and init_type_2 and months_5 then start_time else null end ) as c_use_time_11_init_type_2_months_5_start_time,
count ( case when use_time_6 and init_type_1 and months_3 then start_time else null end ) as c_use_time_6_init_type_1_months_3_start_time,
count ( case when use_time_4 and months_5 then start_time else null end ) as c_use_time_4_months_5_start_time,
count ( case when use_time_5 and months_1 then start_time else null end ) as c_use_time_5_months_1_start_time,
count (distinct  case when use_time_1 and init_type_1 and months_4 then other_cell_phone else null end ) as cd_use_time_1_init_type_1_months_4_other_cell_phone,
count (distinct  case when use_time_2 and init_type_2 and months_2 then other_cell_phone else null end ) as cd_use_time_2_init_type_2_months_2_other_cell_phone,
count (distinct  case when use_time_5 and months_4 then other_cell_phone else null end ) as cd_use_time_5_months_4_other_cell_phone,
