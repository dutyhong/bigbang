中间临时表产生sql：
create table xf_wht_calls_6m_samples_sample_tmp
as 
select *, 
case when use_time >= 0.000000 and use_time < 10.000000 then true else false end as use_time_1,
case when use_time >= 10.000000 and use_time < 20.000000 then true else false end as use_time_2,
case when use_time >= 20.000000 and use_time < 30.000000 then true else false end as use_time_3,
case when use_time >= 30.000000 and use_time < 40.000000 then true else false end as use_time_4,
case when use_time >= 40.000000 and use_time < 50.000000 then true else false end as use_time_5,
case when use_time >= 50.000000 and use_time < 60.000000 then true else false end as use_time_6,
case when use_time >= 60.000000 and use_time < 70.000000 then true else false end as use_time_7,
case when use_time >= 70.000000 and use_time < 80.000000 then true else false end as use_time_8,
case when use_time >= 80.000000 and use_time < 90.000000 then true else false end as use_time_9,
case when use_time >= 90.000000 and use_time < 100.000000 then true else false end as use_time_10,
case when use_time >= 100.000000 and use_time < 110.000000 then true else false end as use_time_11,
case when use_time >= 110.000000 and use_time < 120.000000 then true else false end as use_time_12,
case when init_type='1' then true else false end as init_type_1,
case when init_type='2' then true else false end as init_type_2,
case when (datediff(apply_time, start_time)) <= 30.000000 then true else false end as months_1,
case when (datediff(apply_time, start_time)) <= 60.000000 then true else false end as months_2,
case when (datediff(apply_time, start_time)) <= 90.000000 then true else false end as months_3,
case when (datediff(apply_time, start_time)) <= 120.000000 then true else false end as months_4,
case when (datediff(apply_time, start_time)) <= 150.000000 then true else false end as months_5,
case when (datediff(apply_time, start_time)) <= 180.000000 then true else false end as months_6 from xf_wht_calls_6m_samples_sample
select 
 aid,bad_tag, 
 count ( case when use_time_6 and init_type_2 and months_5 then start_time else null end ) as c_use_time_6_init_type_2_months_5_start_time,
count (distinct  case when use_time_6 and init_type_1 and months_4 then other_cell_phone else null end ) as cd_use_time_6_init_type_1_months_4_other_cell_phone,
count ( case when use_time_11 and init_type_1 and months_6 then start_time else null end ) as c_use_time_11_init_type_1_months_6_start_time,
count ( case when use_time_12 and init_type_2 and months_3 then start_time else null end ) as c_use_time_12_init_type_2_months_3_start_time,
count ( case when use_time_1 and init_type_1 and months_4 then start_time else null end ) as c_use_time_1_init_type_1_months_4_start_time,
