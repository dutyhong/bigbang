count ( case when use_time_4 and months_1 then start_time else null end ) as c_use_time_4_months_1_start_time,
count (distinct  case when use_time_12 and init_type_2 and months_1 then other_cell_phone else null end ) as cd_use_time_12_init_type_2_months_1_other_cell_phone,
count ( case when use_time_7 and init_type_1 and months_3 then start_time else null end ) as c_use_time_7_init_type_1_months_3_start_time,
count ( case when use_time_8 and init_type_2 and months_3 then start_time else null end ) as c_use_time_8_init_type_2_months_3_start_time,
